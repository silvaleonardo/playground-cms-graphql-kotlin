type Page {
    id: Int
    title: String
    summary: String
    body: String
    status: PageStatus
    createdAt: String
    updatedAt: String
    user: User
    comments(page: Int = 1, size: Int = 10): [Comment]
    tags: [Tag]
}

enum PageStatus {
    DRAFT
    PUBLISHED
    HIDDEN
    DELETED
}